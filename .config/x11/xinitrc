#!/bin/sh

pipewire &
pipewire-pulse &
wireplumber &
xrandr --dpi 96 &
xbacklight -set 5 &
xrdb ~/.config/x11/Xresources &
setxkbmap -option caps:escape &
setbg &
mailsync &
xcompmgr &
dunst &
unclutter --timeout 1 --ignore-scrolling &
touchpad &
sxhkd &
clipmenud & 
#blueman-applet &
nm-applet &

[ -s $HOME/.local/state/transmission ] && transmission-daemon

dwmblocks &
exec dwm
