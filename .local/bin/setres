#!/bin/sh
currentres=$(xrandr | grep "\*" | awk '{print $1}')

#if [ "$currentres" = 1280x720 ]
#then
	#xrandr --output eDP1 --mode 1920x1080
#else
	#xrandr --output eDP1 --mode 1280x720
#fi
[ "$currentres" = 1280x720 ] && xrandr --output eDP1 --mode 1920x1080 || xrandr --output eDP1 --mode 1280x720

setbg
